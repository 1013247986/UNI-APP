page {
  height: 100%;
  background: #fff;
}
.main {
  font-family: 'Scour';
  padding: 40rpx 20rpx;
  height: auto;
}
.main .banner {
  height: 300rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-bottom: 40rpx;
}
.main .banner image {
  height: 100%;
  width: 100%;
}
.main ._h3 {
  font-size: 35rpx;
  font-weight: bold;
}
.main .manage {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.main .manage .img {
  width: 90rpx;
  height: 90rpx;
  margin: 0 auto;
  margin-bottom: 10rpx;
}
.main .manage .img image {
  height: 100%;
  width: 100%;
}
.main .manage text {
  color: #444444;
  text-align: center;
  font-size: 25rpx;
}

