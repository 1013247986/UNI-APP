.mian {
  background: #fff;
}
.mian .contant {
  padding: 28rpx;
  border-top: 1rpx solid #e5e5e5;
}
.mian .contant .list-con {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
  color: #999999;
  line-height: 26px;
}
.mian .contant .list-con .text-con {
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
}
.mian .contant .list-con .text-num {
  color: #0099ff;
}
.mian .contant .list-con .text-num .icon-color {
  color: #333333;
}
.mian .contant .list-con .list-inpu {
  width: 136rpx;
  height: 48rpx;
  border: 1rpx solid #a0a0a0;
  border-radius: 10rpx;
  text-align: center;
}
.mian .contant .list-dex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 88%;
}

