.main {
  font-family: 'Scour';
}
.main .select {
  background: #fff;
  padding: 0 20rpx;
}
.main .search {
  background: #fff;
  z-index: 0;
}
.main .waitOut {
  margin-top: 200rpx;
  padding: 0 20rpx;
}
.main .waitOut .content {
  padding: 20rpx 20rpx;
  background-color: #fff;
  margin-top: 10rpx;
  border-radius: 10rpx;
}
.main .waitOut .content .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1rpx solid #e5e5e5;
  font-size: 30rpx;
}
.main .waitOut .content .title ._h3 {
  font-weight: bold;
}
.main .waitOut .content .info .text {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.main .waitOut .content .info .text ._span {
  color: #999999;
}
.main .waitOut .content .info .text .text-num {
  color: #0099ff;
}

